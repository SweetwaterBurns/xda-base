<board>
  <properties>
    <description>
      Board:	Altera Cyclone III Cortex-M1
CPU:	cortexm1
    </description>
    <property name="system-v6-m"/>
  </properties>
  <memory-map>
    <memory-device address="0x00000000" size="32K" type="ram">
      <description>
	ITCM
      </description>
    </memory-device>
    <memory-device address="0x20100000" size="1M" type="ram">
      <description>
	SRAM
      </description>
    </memory-device>
    <memory-device address="0x60000000" size="16M" type="flash" device="cfi">
      <description>
	Flash
      </description>
    </memory-device>
  </memory-map>
<debuggerDefaults>
<debugInterface name="mesp">
<option defaultValue="little" name="endian"/>
<option defaultValue="cortex-m1" name="cpuid"/>
</debugInterface>
</debuggerDefaults>
</board>
